}

/* Estilo para QLineEdit e QDateEdit */
QLineEdit, QDateEdit {
    border: 1px solid #ccc;
    border-radius: 5px;
    font-size: 12pt;
    background-color: #fff;
    color: #333;
    height: 30px;
}

/* Estilo específico para QLineEdit */
QLineEdit {
    padding-left: 5px;
}

/* Estilo específico para QDateEdit */
QDateEdit {
    padding-left: 8px;
}

/* Estilo para os botões de incremento/decremento do QDateEdit */
QDateEdit::up-button, QDateEdit::down-button {
    background-color: #e0e0e0;
    border: none;
    width: 16px;
    height: 16px;
    padding: 0px;
}

QDateEdit::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
}

QDateEdit::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right;
}

QDateEdit::up-arrow {
    image: url(:/icons/up-arrow.png);
    width: 12px;
    height: 12px;
}

QDateEdit::down-arrow {
    image: url(:/icons/down-arrow.png);
    width: 12px;
    height: 12px;
}

/* Estilo para QPushButton */
QPushButton {
    background-color: #e0e0e0;
