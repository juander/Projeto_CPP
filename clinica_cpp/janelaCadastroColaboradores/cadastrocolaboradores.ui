}

/* Estilo para hover em botões específicos */
QPushButton#btnCancelar:hover, QPushButton#btnCancelarPac:hover, QPushButton#btnCancelarCol:hover {
    background-color: #b0b0b0;
}

/* Estilo para botão btnMostrar */
